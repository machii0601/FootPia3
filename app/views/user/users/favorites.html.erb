<div class='container'>
  <div class='row'>
    <div class='col-lg-7 col-md-9 mx-auto align-items-center'>
      <h2 class="text-center mt-4">
        <table class="table">
        <tbody>
          <% @favorite_posts.each do |post| %>
          <tr>
            <td>
              <%= link_to post.title, post_path(post.id) %>
            </td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>